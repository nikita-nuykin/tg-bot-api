{
  "name": "@wagwan/tg-bot-api",
  "version": "1.0.4",
  "description": "NodeJS library for telegram bot api made with TS",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Nikita Nuykin",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/nikita-nuykin/tg-bot-api",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikita-nuykin/tg-bot-api.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/nikita-nuykin/tg-bot-api/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "yarn tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "test": "jest --passWithNoTests",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint && yarn build",
    "preversion": "yarn lint",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "form-data": "4.0.0"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/jest": "27.5.1",
    "@types/node": "17.0.25",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "simple-git-hooks": "2.7.0",
    "ts-jest": "28.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn typecheck && yarn lint && yarn test"
  }
}
